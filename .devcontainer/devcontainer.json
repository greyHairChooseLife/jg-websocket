{
    "name": "Proxylab Development Container",
    "dockerFile": "Dockerfile",
    "remoteUser": "jungle",
    "runArgs": [
        "-p", "8000:8000"
    ],
    "forwardPorts": [8000],
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-vscode.cpptools-extension-pack",
                "ms-vscode.cpptools"
            ],
            "settings": {
                "terminal.integrated.defaultProfile.linux": "bash",
                "C_Cpp.default.includePath": [
                    "/usr/include",
                    "/usr/local/include"
                ],
                "C_Cpp.default.compilerPath": "/usr/bin/gcc"
            }
        }
    }
}